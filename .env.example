# CharityAPI.org Configuration
# Copy this file to .env and add your actual API keys

# API Keys from CharityAPI.org
CHARITY_API_LIVE_KEY=live-your-live-key-here
CHARITY_API_TEST_KEY=test-your-test-key-here

# API Mode: 'test' or 'live'
CHARITY_API_MODE=test

# API Proxy Configuration (Force Multiplication)
# Enable to route all API calls through unified proxy
USE_API_PROXY=false
API_PROXY_URL=http://localhost:8088

# Optional: Override default API endpoint
# CHARITY_API_BASE_URL=https://api.charityapi.org/api/v1

# Rate limiting (optional - handled by proxy when enabled)
# CHARITY_API_RATE_LIMIT=100
# CHARITY_API_RATE_WINDOW=60000

# Cache settings (optional)
# CHARITY_API_CACHE_ENABLED=true
# CHARITY_API_CACHE_TTL=3600
