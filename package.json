{
  "description": "Recovery Compass WFD Sunrise Path",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "cspell": "^9.2.0",
    "cspell-dict-npm": "^1.1.8",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "markdownlint-cli": "^0.45.0"
  },
  "name": "wfd-sunrise-path",
  "scripts": {
    "fix:markdown": "markdownlint '**/*.md' --fix --ignore node_modules",
    "lint:all": "npm run lint:check && npm run lint:yaml",
    "lint:check": "npm run lint:markdown && npm run spell:check",
    "lint:markdown": "markdownlint '**/*.md' --ignore node_modules",
    "lint:yaml": "yamllint '**/*.yml' '**/*.yaml' --config-file .yamllint",
    "spell:check": "cspell '**/*.md' --no-progress",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "version": "1.0.0"
}
