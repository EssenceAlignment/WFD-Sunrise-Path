{
  "description": "Recovery Compass WFD Sunrise Path",
  "type": "module",
  "devDependencies": {
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.18.0",
    "@octokit/rest": "^22.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "babel-jest": "^29.5.0",
    "cspell": "^9.2.0",
    "cspell-dict-npm": "^1.1.8",
    "glob": "^11.0.3",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "markdownlint-cli": "^0.45.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "name": "wfd-sunrise-path",
  "scripts": {
    "test": "jest",
    "lint": "markdownlint '**/*.md' --ignore node_modules --ignore coverage",
    "spell": "cspell \"**/*\" --no-progress",
    "check": "npm run lint && npm run spell",
    "check:es-modules": "node scripts/es-module-migration.js",
    "eval": "openai tools evals ./alignment_tests --model gpt-4o",
    "test:alignment": "npm run eval && echo 'âœ… Alignment passed'",
    "snapshot:objectives": "node scripts/snapshot_objectives.js",
    "scan:gems": "python scripts/scan_hidden_gems.py",
    "fix:security": "node scripts/fix-insecure-patterns.js",
    "fix:typescript": "tsc --noEmit && eslint --fix 'src/**/*.ts' || true",
    "fix:all": "npm run fix:security && npm run fix:typescript && npm run lint:fix || true",
    "lint:fix": "markdownlint '**/*.md' --fix --ignore node_modules --ignore coverage",
    "pre-push": "npm run fix:all && npm test",
    "services:start": "./scripts/service-manager.sh start",
    "services:stop": "./scripts/service-manager.sh stop",
    "services:status": "./scripts/service-manager.sh status",
    "services:health": "./scripts/service-manager.sh health-check",
    "services:chaos": "./scripts/kill-random-service.sh",
    "security:scan": "python3 scripts/security/security_orchestrator.py --scan-only",
    "security:fix": "python3 scripts/security/security_orchestrator.py --fix-all",
    "build": "./build-mobile.sh",
    "cap:sync": "npx cap sync",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:run:ios": "npm run build && npx cap sync && npx cap run ios",
    "cap:run:android": "npm run build && npx cap sync && npx cap run android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android"
  },
  "version": "1.0.0",
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/splash-screen": "^7.0.1",
    "dotenv": "^17.2.1",
    "puppeteer": "^24.15.0"
  },
  "overrides": {
    "@babel/traverse": "latest",
    "form-data": ">=4.0.0",
    "webpack": ">=5.89.0",
    "mongoose": ">=7.5.0",
    "@svgr/plugin-svgo": "latest",
    "@svgr/webpack": "latest",
    "body-parser": ">=1.20.2",
    "braces": "latest",
    "cross-spawn": "latest",
    "css-select": "latest",
    "express": ">=4.18.2",
    "http-proxy-middleware": "latest",
    "nth-check": "latest",
    "path-to-regexp": ">=6.2.1",
    "react-scripts": "latest",
    "rollup": "latest",
    "semver": ">=7.5.2",
    "svgo": "latest",
    "webpack-dev-middleware": "latest",
    "ws": "latest",
    "ip": "latest",
    "socks": "latest",
    "@babel/helpers": "latest",
    "@babel/runtime": "latest",
    "@pmmmwh/react-refresh-webpack-plugin": "latest",
    "ejs": "latest",
    "follow-redirects": "latest",
    "micromatch": "latest",
    "nanoid": "latest",
    "postcss": "latest",
    "resolve-url-loader": "latest",
    "serialize-javascript": "latest",
    "tough-cookie": ">=4.1.3",
    "webpack-dev-server": "latest",
    "word-wrap": ">=1.2.5",
    "brace-expansion": "latest",
    "compression": "latest",
    "cookie": "latest",
    "on-headers": "latest",
    "send": "latest",
    "serve-static": "latest"
  }
}
